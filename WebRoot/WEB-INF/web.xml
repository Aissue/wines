<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>registerService</servlet-name>
  	<servlet-class>com.wines.service.registerService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>registerService</servlet-name>
  	<url-pattern>/register.form</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>verifyService</servlet-name>
  	<servlet-class>com.wines.service.verifyService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>verifyService</servlet-name>
  	<url-pattern>/verify.form</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>loginService</servlet-name>
  	<servlet-class>com.wines.service.loginService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginService</servlet-name>
  	<url-pattern>/login.form</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>checkService</servlet-name>
  	<servlet-class>com.wines.service.checkService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>checkService</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
</web-app>
